        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.ContactNo1, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.ContactNo1, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.ContactNo1)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.ContactNo2, new { @class = "control-label col-md-2" })
