                @Html.TextBoxFor(model => model.Share, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.Share)
            </div>
        </div>*@

    

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    <a href="@Url.Action("Index", "LeadGenerator" )" id="backtolist" title="Click to go back to list">
