@model DAL.LeadGenerator

@{
    ViewBag.Title = "Edit";
}

<h2>Edit Lead Generator</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        
        <hr />
        @Html.ValidationSummary(true)
        <div class="form-group">
            @Html.LabelFor(model => model.LeadGeneratorName, new { @class = "control-label col-md-2" })
